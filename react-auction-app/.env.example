# ============================================================================
# ENVIRONMENT VARIABLES - Cricket Auction App
# Copy this to .env.local and fill in your values
# ============================================================================

# Google Sheets Configuration
VITE_GOOGLE_SHEET_ID=your-google-sheet-id-here
VITE_GOOGLE_API_KEY=your-google-api-key-here

# Google Apps Script Webhook URL
VITE_WEBHOOK_URL=https://script.google.com/macros/s/your-script-id/exec

# Tournament Configuration
# Options: 'default', 'bcc-season-6', 'corporate-league-2025'
VITE_TOURNAMENT_ID=default

# Application Settings
VITE_APP_NAME=Cricket Player Auction
VITE_DEBUG_MODE=false

# Feature Flags
VITE_ENABLE_ROUND_2=true
VITE_ENABLE_AUDIO=true
